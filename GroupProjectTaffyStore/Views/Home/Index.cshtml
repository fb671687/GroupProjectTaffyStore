@{
    ViewBag.Title = "Home Page";
}
<div class="jumbotron" style="background-color:#FFC0CB">
    <img src="~/Images/Cherry.png" width="200" height="200" style="float:right;width:130px;height:130px;padding:20px">
    <img src="~/Images/Orange.png" width="200" height="200" style="float:right;width:130px;height:130px;padding:20px">
